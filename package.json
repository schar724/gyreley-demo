{
  "name": "gyreley-demo",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://schar724.github.io/gyreley-demo/",
  "type": "module",
  "scripts": {
    "get-branch": "git rev-parse --abbrev-ref HEAD",
    "dev": "bash -c 'BRANCH=$(npm run get-branch --silent) && BRANCH=${BRANCH/development/dev} && firebase use gyreley-$BRANCH && vite --mode $(npm run get-branch --silent) && tsr watch'",
    "build": "bash -c 'MODE=$(npm run get-branch --silent) && BRANCH=${MODE/development/dev} && firebase use gyreley-$BRANCH && tsc && vite build'",
    "emulators": "firebase emulators:start --import=./emulatorsScott --export-on-exit=./emulatorsScott",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "bash -c 'BRANCH=$(npm run get-branch --silent) && BRANCH=${BRANCH/development/dev} && firebase use gyreley-$BRANCH && firebase deploy'",
    "emulators:test": "firebase emulators:start --import=./tests/emulator-data --export-on-exit=./tests/emulator-dump",
    "test": "firebase emulators:exec --import=./tests/emulator-data --export-on-exit=./tests/emulator-dump 'npx playwright test'",
    "deploy:github": "gh-pages -d dist --remove '.' --remove 'assets/**/*.png' --remove '**.*.png'"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.6",
    "@heroicons/react": "^2.1.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.52.1",
    "@tanstack/react-query-devtools": "^5.52.1",
    "@tanstack/react-router": "^1.49.2",
    "@tanstack/router-devtools": "^1.49.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/react-native": "^0.73.0",
    "@vis.gl/react-google-maps": "^1.4.2",
    "axios": "^1.7.2",
    "chart.js": "^4.4.7",
    "clsx": "^2.1.1",
    "firebase": "^10.13.0",
    "firebase-admin": "^12.4.0",
    "i": "^0.3.7",
    "jsdom": "^25.0.1",
    "npm": "^10.9.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^7.5.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-native-web": "^0.19.12",
    "tailwind-merge": "^2.5.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss-safe-area": "^0.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@tanstack/router-cli": "^1.49.3",
    "@tanstack/router-plugin": "^1.49.3",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-native-web": "^0.19.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.41",
    "rollup-plugin-visualizer": "^5.13.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.2.2",
    "vite": "^5.2.10",
    "vite-plugin-pwa": "^0.20.0",
    "workbox-core": "^7.1.0"
  },
  "overrides": {
    "sharp": "0.32.6",
    "sharp-ico": "0.1.5"
  }
}
